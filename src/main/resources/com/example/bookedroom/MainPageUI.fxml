<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="350.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bookedroom.MainPageController">
   <children>
      <Pane fx:id="mainImagePane" layoutX="75.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" stylesheets="@style.css">
         <children>
            <ImageView fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
         </children></Pane>
      <Button layoutX="120.0" layoutY="417.0" mnemonicParsing="false" onMouseClicked="#moveMemberLogin" prefHeight="34.0" prefWidth="136.0" text="로그인" textFill="#333333">
         <font>
            <Font name="Malgun Gothic Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="120.0" layoutY="468.0" mnemonicParsing="false" onMouseClicked="#moveRegisterAccount" prefHeight="34.0" prefWidth="136.0" stylesheets="@style.css" text="회원가입" textFill="#333333">
         <font>
            <Font name="Malgun Gothic Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
